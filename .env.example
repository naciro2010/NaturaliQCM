# NaturaliQCM Environment Variables
# Copy this file to .env and configure as needed
# Note: This app is 100% offline, so most settings are optional

# Database Encryption (auto-generated if not provided)
# DB_ENCRYPTION_KEY=

# App Configuration
# APP_ENV=production
# DEBUG_MODE=false

# Feature Flags (for development/testing)
# ENABLE_ANALYTICS=false
# ENABLE_CRASH_REPORTING=false

# Note: Per privacy policy, analytics and crash reporting are DISABLED by default
# and should remain so for production builds
